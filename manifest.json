{
    "manifest_version": 3,
    "name": "New Tab Opener: First Position",
    "version": "1.0",
    "description": "Moves all newly opened tabs to the first position (index 0) in the current window.",
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "new-tab-first-position@janpittner.com"
        },
        "data_collection_permissions": {
            "allows_background_file_access": false,
            "policy_url": null,
            "is_data_collected": false,
            "collection_methods": null,
            "data_collected": null,
            "data_uses": null,
            "is_data_shared": false,
            "data_shared_with": null
        }
    }
}